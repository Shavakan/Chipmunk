{
    "manifest_version": 2,
    "name": "chipmunk_chrome_extension",
    "description": "Print Hello, World! message",
    "version": "1.0.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "<all_urls>"
    ]
}